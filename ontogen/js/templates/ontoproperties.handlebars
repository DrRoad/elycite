<form role="form">
  <div class="form-group">
    <label for="input-name">Name</label>
    <input type="text" class="form-control" id="input-name" value="{{name}}">
  </div>
  <div class="form-group">
    <label for="input-keywords">Keywords</label>
    <input type="text" class="form-control" id="input-keywords" value="{{keywordStr}}">
  </div>

  <button type="button" id="submit-details" data-conceptId={{id}} class="btn btn-default">Submit</button>
</form>
