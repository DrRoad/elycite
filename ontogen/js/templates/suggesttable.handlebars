<thead>
  <tr>
    <th>Name</th>
    <th>Keywords</th>
    <th>Docs</th>
    <th></th>
  </tr>
</thead>
{{#each suggestions}}
<tr data-lid="{{lid}}">
  <td>{{name}}</td>
  <td>{{keywords}}</td>
  <td>{{numDocs}}</td>
  <td><button class="btn btn-primary" id="add-suggested" data-lid="{{lid}}">Add</button></td>
</tr>
{{/each}}

